#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/karlkuhne/Nuxt/karlkuhne.github.io/node_modules/.pnpm/vite@6.3.5_@types+node@24.2.1_jiti@2.4.2_terser@5.43.1_yaml@2.8.0/node_modules/vite/bin/node_modules:/Users/karlkuhne/Nuxt/karlkuhne.github.io/node_modules/.pnpm/vite@6.3.5_@types+node@24.2.1_jiti@2.4.2_terser@5.43.1_yaml@2.8.0/node_modules/vite/node_modules:/Users/karlkuhne/Nuxt/karlkuhne.github.io/node_modules/.pnpm/vite@6.3.5_@types+node@24.2.1_jiti@2.4.2_terser@5.43.1_yaml@2.8.0/node_modules:/Users/karlkuhne/Nuxt/karlkuhne.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/karlkuhne/Nuxt/karlkuhne.github.io/node_modules/.pnpm/vite@6.3.5_@types+node@24.2.1_jiti@2.4.2_terser@5.43.1_yaml@2.8.0/node_modules/vite/bin/node_modules:/Users/karlkuhne/Nuxt/karlkuhne.github.io/node_modules/.pnpm/vite@6.3.5_@types+node@24.2.1_jiti@2.4.2_terser@5.43.1_yaml@2.8.0/node_modules/vite/node_modules:/Users/karlkuhne/Nuxt/karlkuhne.github.io/node_modules/.pnpm/vite@6.3.5_@types+node@24.2.1_jiti@2.4.2_terser@5.43.1_yaml@2.8.0/node_modules:/Users/karlkuhne/Nuxt/karlkuhne.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/vite@6.3.5_@types+node@24.2.1_jiti@2.4.2_terser@5.43.1_yaml@2.8.0/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../.pnpm/vite@6.3.5_@types+node@24.2.1_jiti@2.4.2_terser@5.43.1_yaml@2.8.0/node_modules/vite/bin/vite.js" "$@"
fi
